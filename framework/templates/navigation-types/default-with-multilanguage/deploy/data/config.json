{
	"cacheContents":{
		"src":"{base}js/sw.js",
		"scope":"./"
	},	
	"paths":{
		"cdn":"",
		"base":"",
		"data":"{base}data/",
		"images":"{base}media/images/",
		"videos":"{base}media/videos/"
	},
	"languages":[
		{
			"default":true,
			"iso":"pt-br",
			"sufix":"pt",
			"data-path":"{data}pt-br/"
		},
		{
			"iso":"en-us",
			"sufix":"en",
			"data-path":"{data}en-us/"
		},
		{
			"iso":"es-us",
			"sufix":"es",
			"data-path":"{data}es-es/"
		}
	],
	"navigation":{
		"forceHashBang":false,
		"startBefore":true,
		"autoStart":true,
		"instantiateViews":true,
		"defaultView":"home"
	},
	"conditions": {
        "mobile":{
            "browser":{
            	"mobile": true
            }
        },
        "desktop":{
            "browser":{
            	"desktop": true
            }
        }
    },	
	"views":[
		{
			"id":"home",
			"class":"template-home-view",
			"route":"/",
			"content":[
				{
					"condition":"desktop",
					"file":"{language-data}home.json",
					"cache":true,
					"testestes":"teste"

				}
			],
			"destroyable":true,
			"loadContent":true,
			"cache":true
		},
		{
			"id":"sub1",
			"class":"template-sub-view",
			"content":"{language-data}sub1.json",
			"route":"/sub-view-01",
			"parentView":"home",
			"destroyable":true,
			"loadContent":true,
			"cache":true
			
		},
		{
			"id":"sub2",
			"class":"template-sub-view",
			"content":"{language-data}sub2.json",
			"route":"/sub-view-02",
			"parentView":"sub1",
			"destroyable":true,
			"loadContent":true,
			"cache":true
		},
		{
			"id":"sub3",
			"class":"template-sub-view",
			"content":"{language-data}sub3.json",
			"route":"/sub-view-03",
			"destroyable":true,
			"loadContent":true,
			"cache":true
		},
		{
			"id":"sub4",
			"class":"template-sub-view",
			"content":"{language-data}sub4.json",
			"route":"/sub-view-04",
			"destroyable":true,
			"loadContent":true,
			"cache":true
		},
		{
			"id":"sub5",
			"class":"template-sub-view",
			"content":"{language-data}sub5.json",
			"route":"/sub-view-05",
			"destroyable":true,
			"loadContent":true,
			"cache":true
		}
	],
	"required":
	{
		"preloader":
		[
			{
				"id": "preloader-css",
				"src": "{base}css/preloader.css",
				"cache":true
			},
			{
				"id":"preloader-content",
				"content":"{language-data}preloader.json",
				"cache":true
			}
		],
		"core":
		[
			{
				"id":"vendors",
				"src":"{base}js/vendors.js",
				"cache":true
			}
		],
		"main":
		[
			{
				"id": "main-css",
				"src": "{base}css/main.css",
				"cache":true
			},
			{
				"id": "main-js",
				"src": [
					{
						"condition":"desktop",
						"file":"{base}js/main.js"
					}
				],
				"cache":true
			},
			{
				"content":[
					{
						"condition":"desktop",
						"file":"{language-data}main.json",
						"cache":true
					}
				],
				"cache":true
			}
		]
	}
}
